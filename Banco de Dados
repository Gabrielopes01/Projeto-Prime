CREATE DATABASE PrimeMoveis;
USE PrimeMoveis;

CREATE TABLE Usuario(
    Id INT PRIMARY KEY IDENTITY,
    Nome VARCHAR(200) NOT NULL,
    Email VARCHAR(200) NOT NULL,
    Data DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP,
    Senha VARCHAR(900) NOT NULL,
    Adm INT NOT NULL
);


